{
    "name": "desktop-os-for-admin",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "type": "commonjs",
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "postinstall": "ts-node -P ./scripts/patch/tsconfig.json ./scripts/patch/index.ts",
        "pm2": "pm2 start --name nest dist/server/main.js",
        "build": "yarn build:client && yarn build:render && yarn build:server",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:render": "vite build --ssr src/client/entry-server.ts --outDir dist/client/render",
        "build:server": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "dev": "nest start --watch",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "pm2 start ecosystem.config.js",
        "lint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest--runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "generate:request-code": "ts-node -P ./scripts/generate-request-code/tsconfig.json ./scripts/generate-request-code/index.ts -c=$CONTROLLER",
        "generate:graphql-code": "zeus http://localhost:3000/graphql ./src/client/graphql",
        "commit": "git-cz",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/icons-vue": "~6.0.1",
        "@gopowerteam/http-request": "^0.2.3",
        "@gopowerteam/vue-modal": "^0.1.7",
        "@nestjs/common": "^7.6.17",
        "@nestjs/core": "^7.6.17",
        "@nestjs/graphql": "~7.10.2",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/mongoose": "^7.2.4",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.6.17",
        "@nestjs/typeorm": "~7.1.5",
        "ant-design-vue": "^2.2.0-beta.2",
        "apollo-server-express": "^2.24.1",
        "axios": "~0.21.1",
        "better-sqlite3": "^7.4.1",
        "cache-manager": "~3.4.1",
        "class-transformer": "~0.4.0",
        "class-validator": "^0.13.1",
        "graphql": "~15.5.0",
        "graphql-request": "~3.4.0",
        "graphql-tools": "~7.0.4",
        "graphql-zeus": "^3.0.7",
        "http-proxy": "~1.18.1",
        "less": "~4.1.1",
        "lodash": "~4.17.21",
        "lodash-es": "^4.17.21",
        "log4js": "^6.3.0",
        "mongodb": "^3.6.8",
        "mongoose": "^5.12.10",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pug": "~3.0.2",
        "ramda": "^0.27.1",
        "read-excel-file": "^5.1.0",
        "read-yaml-file": "~2.1.0",
        "reflect-metadata": "~0.1.13",
        "rimraf": "~3.0.2",
        "rxjs": "^7.1.0",
        "typeorm": "~0.2.31",
        "vue": "^3.1.1",
        "vue-router": "^4.0.8",
        "vuex": "4.0.0",
        "vuex-persistedstate": "~4.0.0-beta.3"
    },
    "devDependencies": {
        "@apollo/gateway": "^0.28.1",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@nestjs/cli": "~7.6.0",
        "@nestjs/schematics": "~7.3.1",
        "@nestjs/testing": "^7.6.17",
        "@prettier/plugin-pug": "^1.15.2",
        "@types/cache-manager": "~3.4.0",
        "@types/express": "~4.17.8",
        "@types/http-proxy": "^1.17.6",
        "@types/jest": "~26.0.15",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^15.6.0",
        "@types/passport-jwt": "^3.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/ramda": "^0.27.40",
        "@types/supertest": "~2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "@vitejs/plugin-vue": "~1.2.0",
        "@vitejs/plugin-vue-jsx": "~1.1.2",
        "@vue/compiler-sfc": "^3.1.1",
        "@vue/server-renderer": "^3.1.1",
        "@vuedx/typecheck": "^0.7.1",
        "@vuedx/typescript-plugin-vue": "^0.7.1",
        "autoprefixer": "~10.2.5",
        "commitizen": "^4.2.3",
        "compression": "~1.7.4",
        "cross-env": "~7.0.3",
        "cz-conventional-changelog-emoji": "^0.1.0",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-vue": "^7.9.0",
        "handlebars": "~4.7.7",
        "husky": "^6.0.0",
        "jest": "~26.6.3",
        "lint-staged": ">=10",
        "pm2": "^4.5.6",
        "postcss": "^8.3.0",
        "prettier": "^2.3.0",
        "stylelint": "^13.13.0",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-plugin-stylus": "^0.11.0",
        "stylus": "~0.54.8",
        "supertest": "~6.1.3",
        "tailwindcss": "^2.1.2",
        "ts-jest": "~26.5.3",
        "ts-loader": "^9.2.2",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "~3.9.0",
        "typescript": "^4.3.2",
        "vite": "^2.3.3",
        "vite-svg-loader": "~1.5.1",
        "vite-tsconfig-paths": "^3.3.12"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog-emoji"
        }
    },
    "lint-staged": {
        "*.ts!*.d.ts": [
            "eslint  --fix",
            "prettier --write",
            "git add ."
        ],
        "*.vue": [
            "eslint  --fix",
            "prettier --write",
            "git add ."
        ],
        "*.js": "eslint --cache --fix",
        "*.css": "stylelint --fix"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "~.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
