{
    "name": "desktop-os-for-admin",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "type": "commonjs",
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "postinstall": "ts-node -P ./scripts/patch/tsconfig.json ./scripts/patch/index.ts",
        "pm2": "pm2 start --name nest dist/server/main.js",
        "build": "yarn build:client && yarn build:render && yarn build:server",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:render": "vite build --ssr src/client/entry-server.ts --outDir dist/client/render",
        "build:server": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "dev": "nest start --watch",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "pm2 start ecosystem.config.js",
        "lint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest--runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "generate:request-code": "ts-node -P ./scripts/generate-request-code/tsconfig.json ./scripts/generate-request-code/index.ts -c=$CONTROLLER",
        "generate:graphql-code": "zeus http://localhost:3000/graphql ./src/client/graphql",
        "commit": "git-cz",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/icons-vue": "6.0.1",
        "@gopowerteam/http-request": "0.2.5",
        "@gopowerteam/vue-modal": "0.1.7",
        "@nestjs/common": "8.0.4",
        "@nestjs/core": "8.0.4",
        "@nestjs/graphql": "8.0.2",
        "@nestjs/jwt": "8.0.0",
        "@nestjs/mongoose": "8.0.0",
        "@nestjs/passport": "8.0.0",
        "@nestjs/platform-express": "8.0.4",
        "@nestjs/typeorm": "8.0.1",
        "ant-design-vue": "2.2.2",
        "apollo-server-express": "2.25.2",
        "axios": "0.21.1",
        "better-sqlite3": "7.4.1",
        "cache-manager": "3.4.4",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "graphql": "15.5.1",
        "graphql-request": "3.4.0",
        "graphql-tools": "7.0.5",
        "graphql-zeus": "3.1.0",
        "http-proxy": "1.18.1",
        "less": "4.1.1",
        "lodash": "4.17.21",
        "lodash-es": "4.17.21",
        "log4js": "6.3.0",
        "mongodb": "4.0.0",
        "mongoose": "5.13.3",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "pug": "3.0.2",
        "ramda": "0.27.1",
        "read-excel-file": "5.2.8",
        "read-yaml-file": "2.1.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.2.0",
        "typeorm": "0.2.34",
        "vue": "3.1.5",
        "vue-router": "4.0.10",
        "vuex": "4.0.2",
        "vuex-persistedstate": "4.0.0-beta.3"
    },
    "devDependencies": {
        "@apollo/gateway": "0.33.0",
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@nestjs/cli": "8.0.2",
        "@nestjs/schematics": "8.0.2",
        "@nestjs/testing": "8.0.4",
        "@prettier/plugin-pug": "1.16.1",
        "@types/cache-manager": "3.4.2",
        "@types/express": "4.17.13",
        "@types/http-proxy": "1.17.7",
        "@types/jest": "26.0.24",
        "@types/lodash-es": "4.17.4",
        "@types/node": "16.3.3",
        "@types/passport-jwt": "3.0.6",
        "@types/passport-local": "1.0.34",
        "@types/ramda": "0.27.44",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "4.28.3",
        "@vitejs/plugin-vue": "1.2.5",
        "@vitejs/plugin-vue-jsx": "1.1.6",
        "@vue/compiler-sfc": "3.1.5",
        "@vue/server-renderer": "3.1.5",
        "@vuedx/typecheck": "0.7.4",
        "@vuedx/typescript-plugin-vue": "0.7.4",
        "autoprefixer": "10.3.1",
        "commitizen": "4.2.4",
        "compression": "1.7.4",
        "cross-env": "7.0.3",
        "cz-conventional-changelog-emoji": "0.1.0",
        "eslint": "7.31.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-vue": "7.14.0",
        "handlebars": "4.7.7",
        "husky": "7.0.1",
        "jest": "27.0.6",
        "lint-staged": "11.0.1",
        "pm2": "5.1.0",
        "postcss": "8.3.5",
        "prettier": "2.3.2",
        "stylelint": "13.13.1",
        "stylelint-config-recommended": "5.0.0",
        "stylelint-plugin-stylus": "0.11.0",
        "stylus": "0.54.8",
        "supertest": "6.1.3",
        "tailwindcss": "2.2.4",
        "ts-jest": "27.0.3",
        "ts-loader": "9.2.3",
        "ts-morph": "^11.0.3",
        "ts-node": "10.1.0",
        "tsconfig-paths": "3.10.1",
        "typescript": "4.3.5",
        "vite": "2.4.2",
        "vite-svg-loader": "2.1.0",
        "vite-tsconfig-paths": "3.3.13"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog-emoji"
        }
    },
    "lint-staged": {
        "*.ts!*.d.ts": [
            "eslint  --fix",
            "prettier --write",
            "git add ."
        ],
        "*.vue": [
            "eslint  --fix",
            "prettier --write",
            "git add ."
        ],
        "*.js": "eslint --cache --fix",
        "*.css": "stylelint --fix"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "~.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
